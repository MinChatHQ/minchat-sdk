{
  "version": "3.4.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@minchat/js",
  "author": "MinChat",
  "module": "dist/js.esm.js",
  "size-limit": [
    {
      "path": "dist/js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/js.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/socket.io": "^3.0.2",
    "axios-mock-adapter": "^1.21.5",
    "cuid": "^3.0.0",
    "husky": "^8.0.1",
    "size-limit": "^8.1.0",
    "ts-loader": "^9.4.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
